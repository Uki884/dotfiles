[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[init]
  defaultBranch = main
[alias]
	co = checkout
[grep]
  lineNumber = true
[push]
  default = current
[pull]
  ff = only